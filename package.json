{
  "name": "easy-cluster",
  "version": "0.1.3",
  "description": "A cluster daemon with a rest interface for your app.",
  "bin": {
    "ecluster": "./bin/easy-cluster.js"
  },
  "preferGlobal": true,
  "scripts": {
    "jshint": "jshint server.js lib bin && jshint --config .jshintrc-test test",
    "mocha": "mocha -w",
    "integration": "mocha './integration/*.js'",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "report": "istanbul report cobertura",
    "test": "npm run jshint && npm run cover && npm run integration"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdevel/node-easy-cluster.git"
  },
  "keywords": [
    "cluster",
    "daemon",
    "manager"
  ],
  "contributors": [
    {
      "name": "Joe Spencer",
      "email": "js.developer.undefined@gmail.com",
      "url": "https://github.com/jsdevel"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsdevel/node-easy-cluster/issues"
  },
  "devDependencies": {
    "mocha": "~1.17.1",
    "istanbul": "~0.2.4",
    "proxyquire": "~0.5.2",
    "sinon": "~1.8.2",
    "jshint": "~2.4.3",
    "glob": "~3.2.8",
    "should": "~3.3.0",
    "request": "~2.34.0"
  },
  "dependencies": {
    "express": "~3.4.8",
    "minimist": "0.0.7",
    "express-namespace": "~0.1.1",
    "async": "~0.2.10",
    "express-crud": "0.0.2",
    "err-handler": "0.0.0"
  }
}
